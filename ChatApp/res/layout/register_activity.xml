<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:customfontdemo="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff" >

    <ScrollView
        android:id="@+id/scrollview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <RelativeLayout
            android:id="@+id/biglayout"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="20dp"
            android:paddingBottom="35dp" >

            <RelativeLayout
                android:id="@+id/TRAINEELIST_MAIN_LAYOUT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:visibility="gone" >

                <Button
                    android:id="@+id/TRAINEE_CANCLE_BTN"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_margin="10dp"
                    android:background="@android:color/transparent"
                    android:text="CANCEL"
                    android:textSize="20dp"
                    android:textStyle="bold" />

                <ImageButton
                    android:id="@+id/TRAINEE_ADD_BTN"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_margin="10dp"
                    android:background="@drawable/plusicon"
                    android:visibility="gone" />

                <EditText
                    android:id="@+id/TRAINEE_SEARCH_EDITTEXT"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_toLeftOf="@+id/TRAINEE_ADD_BTN"
                    android:layout_toRightOf="@+id/TRAINEE_CANCLE_BTN"
                    android:background="@drawable/roundes_edittext_border"
                    android:focusable="true"
                    android:hint="search" />

                <RelativeLayout
                    android:id="@+id/TRAINEELIST_LAYOUT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/TRAINEE_SEARCH_EDITTEXT" >
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/MAILALL_LAYOUT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <RelativeLayout
                    android:id="@+id/titlelayout"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center" >

                    <Button
                        android:id="@+id/cancelbtn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginLeft="10dp"
                        android:background="@android:color/transparent"
                        android:clickable="True"
                         android:paddingTop="10dp"
                        android:gravity="center"
                        android:text="@string/icon_BACKBTN"
                        android:textColor="@android:color/black"
                        android:textSize="40dp" />

                        <com.MWC.chatapp.MyTextView
                            android:id="@+id/registertxt"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginTop="10dp"
                            android:gravity="center"
                            android:paddingBottom="10dp"
                            android:paddingTop="10dp"
                            android:text="REGISTER"
                            android:textColor="#000000"
                            android:textSize="25dp"
                            customfontdemo:fontName="calibri.ttf" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image1layout888"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/titlelayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/namelayout"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_below="@+id/image1layout888"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="20dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/nametxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="YOUR NAME"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/nameedt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/nametxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:gravity="right"
                        android:hint="FirstName"
                        android:imeOptions="actionDone"
                        android:paddingRight="5dp"
                        android:singleLine="true"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/imagelayout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/namelayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/name1layout"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_below="@+id/imagelayout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/lastnametxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="LAST NAME"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/lastnameedt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/lastnametxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:gravity="right"
                        android:hint="LastName"
                        android:imeOptions="actionDone"
                        android:paddingRight="5dp"
                        android:singleLine="true"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image1layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/name1layout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/agentcodelayout"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_below="@+id/image1layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/agentcodetxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="AGENT CODE"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/agentcodeedt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/agentcodetxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:gravity="right"
                        android:hint="Agent ID"
                        android:imeOptions="actionDone"
                        android:paddingRight="5dp"
                        android:singleLine="true"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image3layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/agentcodelayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/usernamelayout"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_below="@+id/image3layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/usernametxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="USER NAME"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/usernameedt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/usernametxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:gravity="right"
                        android:hint="Username"
                        android:imeOptions="actionDone"
                        android:paddingRight="5dp"
                        android:singleLine="true"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image4layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/usernamelayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/passwordlayout"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_below="@+id/image4layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/passwordtxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="PASSWORD"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/passwordedt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/passwordtxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:gravity="right"
                        android:hint="Password"
                        android:imeOptions="actionDone"
                        android:inputType="textPassword"
                        android:paddingRight="5dp"
                        android:singleLine="true"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image5layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/passwordlayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/confirmpasswordlayout"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_below="@+id/image5layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/confirmpasswordtxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="CONFIRM PASSWORD"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/confirmpasswordedt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/confirmpasswordtxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:gravity="right"
                        android:hint="Confirm Password"
                        android:imeOptions="actionDone"
                        android:inputType="textPassword"
                        android:paddingRight="5dp"
                        android:singleLine="true"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image6layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/confirmpasswordlayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/emaillayout"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_below="@+id/image6layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/emailtxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="EMAIL"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/emailedt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/emailtxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:gravity="right"
                        android:hint="Email"
                        android:imeOptions="actionDone"
                        android:paddingRight="5dp"
                        android:singleLine="true"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image7layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/emaillayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/levellayout"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_below="@+id/image7layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/leveltxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="LEVEL"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/leveledt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/leveltxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:focusableInTouchMode="false"
                        android:gravity="right"
                        android:hint="Select Type"
                        android:paddingRight="5dp"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image8layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/levellayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/uplineceolayout"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:layout_below="@+id/image8layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/uplineceotxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="UPLINE\nCEO/EVC"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/uplineceoedt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/uplineceotxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:focusableInTouchMode="false"
                        android:gravity="right"
                        android:hint="Select Your CEO/EVC"
                        android:paddingRight="5dp"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image9layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/uplineceolayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/uplinesmdlayout"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:layout_below="@+id/image9layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/uplinesmdtxt"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="UPLINE\nSMD/EMD"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/uplinesmdedt"
                        android:layout_width="200dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/uplinesmdtxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:focusableInTouchMode="false"
                        android:gravity="right"
                        android:hint="Select Your SMD/EMD"
                        android:paddingRight="5dp"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image2layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/uplinesmdlayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/authocodelayout"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_below="@+id/image2layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="13dp"
                    android:gravity="left" >

                    <com.MWC.chatapp.MyTextView
                        android:id="@+id/authocodetxt"
                        android:layout_width="125dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="6dp"
                        android:gravity="center|left"
                        android:text="PROMO CODE"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        customfontdemo:fontName="calibri.ttf" />

                    <EditText
                        android:id="@+id/authocodeedt"
                        android:layout_width="180dp"
                        android:layout_height="fill_parent"
                        android:layout_alignBaseline="@+id/authocodetxt"
                        android:layout_alignParentRight="true"
                        android:background="#00000000"
                        android:gravity="right"
                        android:hint="Optional"
                        android:imeOptions="actionDone"
                        android:paddingRight="5dp"
                        android:singleLine="true"
                        android:textColor="#000000" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/image10layout"
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/authocodelayout"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:layout_marginTop="3dp"
                    android:background="#000000" >
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/buttonlayout"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/image10layout"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="1dp"
                    android:gravity="center" >

                    <Button
                        android:id="@+id/createaccbtn"
                        android:layout_width="250dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="5dp"
                        android:background="#90EE90"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="CREATE AN ACCOUNT"
                        android:textColor="#ffffff"
                        android:textSize="20dp" />
                    
                     <Button
                    android:id="@+id/newcancelbtn"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:layout_below="@+id/createaccbtn"
                    android:layout_centerHorizontal="true"
                    android:background="#90EE90"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="CANCEL"
                    android:textColor="#ffffff"
                    android:textSize="20dp" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_below="@+id/buttonlayout" />
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>

    <SlidingDrawer
        android:id="@+id/Events_slidingDrawer"
        android:layout_width="fill_parent"
        android:layout_height="250dp"
        android:layout_alignParentBottom="true"
        android:content="@+id/content"
        android:handle="@+id/handle"
        android:visibility="gone" >

        <Button
            android:id="@+id/handle"
            android:layout_width="0dp"
            android:layout_height="0dp" />

        <RelativeLayout
            android:id="@+id/content"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@drawable/sdbg"
            android:padding="10dp" >

            <RelativeLayout
                android:id="@+id/EventsSDRelativeLayout"
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:gravity="center_vertical|center" >

                <Button
                    android:id="@+id/Events_Wheel_Cancle_Button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="Cancel" />

                <TextView
                    android:id="@+id/Events_WheelHeader_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text=""
                    android:textColor="@android:color/white" />

                <Button
                    android:id="@+id/Events_Wheel_Done_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="Done" />
            </RelativeLayout>

            <kankan.wheel.widget.WheelView
                android:id="@+id/Events_Wheel"
                android:layout_width="fill_parent"
                android:layout_height="210dp"
                android:layout_below="@+id/EventsSDRelativeLayout" />
        </RelativeLayout>
    </SlidingDrawer>

</RelativeLayout>